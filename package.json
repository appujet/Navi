{
  "name": "navi",
  "version": "1.0.0",
  "description": "A versatile Discord bot with a captivating blend of musical magic and powerful moderation capabilities.",
  "type": "module",
  "exports": [
    "./dist/index.js"
  ],
  "scripts": {
    "start": "npm run start:bot",
    "start:bot": "npm run build && node --enable-source-maps dist/index.js",
    "build": "tsc --project tsconfig.json",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "pm2:start": "pm2 start process.json",
    "pm2:stop": "pm2 stop process.json",
    "pm2:restart": "pm2 restart process.json",
    "pm2:delete": "pm2 delete process.json"
  },
  "keywords": [],
  "author": "Blacky",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/node": "^20.4.10",
    "@types/signale": "^1.4.4",
    "prisma": "^5.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "canvas": "^2.11.2",
    "discord.js": "^14.12.1",
    "dotenv": "^16.3.1",
    "prisma-redis-middleware": "^4.8.0",
    "shoukaku": "^3.4.0",
    "signale": "^1.4.0",
    "superjson": "^1.13.1",
    "undici": "^5.23.0"
  },
  "signale": {
    "displayScope": true,
    "displayBadge": true,
    "displayDate": true,
    "displayFilename": true,
    "displayLabel": true,
    "displayTimestamp": true,
    "underlineLabel": true
  }
}
